---
import '../styles/home/main.css'
import '../styles/home/article.css'
import NavBar from '../components/home/NavBar.astro'
import Footer from '../components/home/Footer.astro'


export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon"  href="images/home/admin.jpg" />
		
		
		<title>{title}</title>
	</head>
	<body>
		<NavBar/>
		<section class='container--main article--container'>
			<slot />
		</section>
		<Footer />
	</body>
</html>
