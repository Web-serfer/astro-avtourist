---
import '../styles/home/main.css'
import Favicons from '../components/home/Favicons.astro'
import Header from '../components/home/Header.astro'
import Footer from '../components/home/Footer.astro'

export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="ru">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1">		
		<Favicons />
		
		<meta name="description" content="страница наиболее частых вопросов и ответов в области автоправа" />

    <meta name="keywords" content="вопросы и ответы автоюриста, задать вопрос автоюристу, ответы автоюриста, консультация автоюриста" />
		<!-- Canonical link -->
		<link rel="canonical" href="https://avtourist-surgut.ru/faq">		
		<title>{title}</title>
	</head>
	<body>
		<Header />
		<section class='container'>
			<slot />
		</section>
		<Footer />
	</body>
</html>






